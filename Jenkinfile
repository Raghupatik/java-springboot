pipeline {
    agent any
          stage('Build') {
            steps {
                echo 'Build'
                sh "mvn package"
            }
        }
        stage('Test') {
            steps {
                echo 'Test'
                sh "mvn Test"
            }
        }
         
        stage('Deploy Dev') {
            steps {
                echo 'Deploy Dev'
            }
        }
    }
}



















