pipeline{
    agent any
    stage("Build"){
         steps{
                 echo "this is build stage"
         }
    }
 stage("Test"){
         steps{
                    echo "this is test stage"
         }
    }
    stage("Sonarqube"){
         steps{ 
                   echo "this is Sonarqube"
            
         }
    }
    stage("Push to artifactory"){
         steps{
                 echo " pust to artifactory "
         }
 stage("Deploy to QA"){
         steps{
                 echo "Deploy to QA"
         }
  
      }
      stage("Deploy to Prod"){
         steps{

                  echo "Deploy to  PROD"
         }
    }
    post{

        failure{
            echo "build failed"
        }
        sucess{
            echo " build sucess"
        }
        
    }
}
