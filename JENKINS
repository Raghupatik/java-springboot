pipeline {
    agent any 
    stages {
        stage('Build') {
            steps {
                echo 'Build'               
            }
        }
        stage('Test') {
            steps {
                echo 'Tst1'
            }
        }
        stage('Push to artifactory') {
            steps {
                echo 'Push to artifactory'
            }
        }
        stage('Test2') {
            steps{
                echo 'QA'
            }
        }

        stage('Deploy to Prod') {
            steps {
                echo 'Deploy to Prod'
            }
        }
    }
    post {
      failure {
        echo 'Failed'
      }
      success {
        echo 'Success'
      }
      aborted {
        echo 'aborted'
      }
    }
}